.main_table_body_tr{
	display: flex;
}
.main_table_body_tr>view{
	flex: 1;
	border: 1px solid #eee;
	/* border-top: none; */
	margin: 4px 0;
	padding: 4px;
}
.main_table_body_td_title{
	background-color: #eee;
}
.alone view{
	padding: 4px;
}
.tab{
	display: flex;
	padding: 8px 0;
	border-bottom: 1px solid #eee;
}
.tab_left{
	flex: 1;
}
.tab_right{
	flex: 0 0 80px;
}
.report_warpper{
	height: calc(100vh - 44px);
}
.main{
	height:calc(100% - 44px);
	overflow: auto;
}
